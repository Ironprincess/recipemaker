generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ingredient {
  id        String   @id @default(cuid())
  name      String
  quantity  Float
  unit      String
  expiresAt DateTime?
  category  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
